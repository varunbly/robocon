        </geometry>
      </visual>
    </link>

    <joint name="back_left_wheel_joint" type="revolute">
      <parent>base_link</parent>  <child>back_left_wheel</child> 
      <axis>
        <xyz>1 0 0</xyz>           <limit>
          <lower>-1e16</lower>     <upper>1e16</upper>
        </limit>
      </axis>
    </joint>

    <link name="front_left_wheel">
      <pose>0.82 -0.700 -0.075 1.5707 0 0</pose> <inertial>
        <mass>0.5</mass>
        <inertia>
          <ixx>0.001</ixx> <ixy>0</ixy> <ixz>0</ixz>
