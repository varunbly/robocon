      <pose>0.82 0 -0.075 1.5707 0 0</pose> <inertial>
        <mass>0.5</mass>
        <inertia>
          <ixx>0.001</ixx> <ixy>0</ixy> <ixz>0</ixz>
          <iyy>0.001</iyy> <iyz>0</iyz>
