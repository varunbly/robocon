          <lower>-1e16</lower>     <upper>1e16</upper>
        </limit>
      </axis>
    </joint>

    <link name="front_right_wheel">
      <pose>-0.01 -0.700 -0.075 1.5707 0 0</pose> <inertial>
        <mass>0.5</mass>
        <inertia>
          <ixx>0.001</ixx> <ixy>0</ixy> <ixz>0</ixz>
