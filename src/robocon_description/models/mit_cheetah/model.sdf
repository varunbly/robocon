<?xml version="1.0"?>
<sdf version="1.8">
  <model name="mit_cheetah">
    <pose>0 0 0.35 0 0 0</pose>
    
    <link name="base_link">
      <inertial>
        <mass>10</mass>
        <inertia>
          <ixx>0.1</ixx><ixy>0</ixy><ixz>0</ixz>
          <iyy>0.5</iyy><iyz>0</iyz>
          <izz>0.5</izz>
        </inertia>
      </inertial>
      <visual name="body">
        <geometry>
          <box><size>0.4 0.2 0.1</size></box>
        </geometry>
        <material>
          <ambient>0.3 0.3 0.3 1</ambient>
          <diffuse>0.3 0.3 0.3 1</diffuse>
        </material>
      </visual>
      <collision name="body_collision">
        <geometry>
          <box><size>0.4 0.2 0.1</size></box>
        </geometry>
      </collision>
    </link>

    <!-- Front Right Leg -->
    <link name="fr_thigh">
      <pose>0.15 -0.1 -0.05 0 0 0</pose>
      <inertial>
        <mass>0.5</mass>
        <inertia>
          <ixx>0.001</ixx><ixy>0</ixy><ixz>0</ixz>
          <iyy>0.001</iyy><iyz>0</iyz>
          <izz>0.0005</izz>
        </inertia>
      </inertial>
      <visual name="thigh">
        <pose>0 0 -0.075 0 0 0</pose>
        <geometry>
          <cylinder><radius>0.02</radius><length>0.15</length></cylinder>
        </geometry>
        <material>
          <ambient>0.8 0.2 0.2 1</ambient>
          <diffuse>0.8 0.2 0.2 1</diffuse>
        </material>
      </visual>
      <collision name="thigh_collision">
        <pose>0 0 -0.075 0 0 0</pose>
        <geometry>
          <cylinder><radius>0.02</radius><length>0.15</length></cylinder>
        </geometry>
      </collision>
    </link>

    <link name="fr_shin">
      <pose>0.15 -0.1 -0.2 0 0 0</pose>
      <inertial>
        <mass>0.3</mass>
        <inertia>
          <ixx>0.001</ixx><ixy>0</ixy><ixz>0</ixz>
          <iyy>0.001</iyy><iyz>0</iyz>
          <izz>0.0003</izz>
        </inertia>
      </inertial>
      <visual name="shin">
        <pose>0 0 -0.075 0 0 0</pose>
        <geometry>
          <cylinder><radius>0.015</radius><length>0.15</length></cylinder>
        </geometry>
        <material>
          <ambient>0.6 0.1 0.1 1</ambient>
          <diffuse>0.6 0.1 0.1 1</diffuse>
        </material>
      </visual>
      <collision name="shin_collision">
        <pose>0 0 -0.075 0 0 0</pose>
        <geometry>
          <cylinder><radius>0.015</radius><length>0.15</length></cylinder>
        </geometry>
      </collision>
      <collision name="foot">
        <pose>0 0 -0.15 0 0 0</pose>
        <geometry>
          <sphere><radius>0.025</radius></sphere>
        </geometry>
        <surface>
          <friction>
            <ode>
              <mu>1.0</mu>
              <mu2>1.0</mu2>
            </ode>
          </friction>
        </surface>
      </collision>
    </link>

