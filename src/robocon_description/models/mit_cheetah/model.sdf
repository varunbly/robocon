      <child>rl_thigh</child>
      <axis><xyz>0 1 0</xyz><limit><lower>-1.0</lower><upper>1.0</upper></limit></axis>
    </joint>

    <!-- Knee Joints -->
    <joint name="fr_knee" type="revolute">
      <parent>fr_thigh</parent>
      <child>fr_shin</child>
      <axis><xyz>0 1 0</xyz><limit><lower>-2.5</lower><upper>0</upper></limit></axis>
    </joint>

    <joint name="fl_knee" type="revolute">
