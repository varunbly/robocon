    <!-- Hip Joints -->
    <joint name="fr_hip" type="revolute">
      <parent>base_link</parent>
      <child>fr_thigh</child>
      <axis><xyz>0 1 0</xyz><limit><lower>-1.0</lower><upper>1.0</upper></limit></axis>
    </joint>

    <joint name="fl_hip" type="revolute">
      <parent>base_link</parent>
      <child>fl_thigh</child>
      <axis><xyz>0 1 0</xyz><limit><lower>-1.0</lower><upper>1.0</upper></limit></axis>
    </joint>

    <joint name="rr_hip" type="revolute">
      <parent>base_link</parent>
      <child>rr_thigh</child>
      <axis><xyz>0 1 0</xyz><limit><lower>-1.0</lower><upper>1.0</upper></limit></axis>
    </joint>

    <joint name="rl_hip" type="revolute">
